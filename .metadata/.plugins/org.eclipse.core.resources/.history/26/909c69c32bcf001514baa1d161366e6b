package geneticAlgorithms.test;

import java.awt.Point;
import java.util.Vector;

import javax.swing.SwingWorker;

public class TestObserver extends SwingWorker{
	private TSP_GA_ConcreteSubject tspConcrete;
	private ResultsObserver resOb;
	private Vector<Vector<Point>> data;

	@Override
	protected Object doInBackground() throws Exception {
		data=new Vector<Vector<Point>>();
		resOb=new ResultsObserver(data);
		tspConcrete.addObserver(resOb);
		tspConcrete.startTSP();
		return null;
	}
	
	public void updateProgress(int n){
		this.setProgress(n);
	}

}
