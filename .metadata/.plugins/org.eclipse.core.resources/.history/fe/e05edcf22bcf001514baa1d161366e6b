package testTSPGA;

import java.awt.Point;
import java.util.Vector;

import javax.swing.SwingWorker;

public class FinalTest extends SwingWorker{
	private Vector<Vector<Point>> resultsData;
	
	public FinalTest(Vector<Vector<Point>> resData){
		resultsData=resData;
	}

	@Override
	protected Object doInBackground() throws Exception {
		TSP_GA_Worker work=new TSP_GA_Worker(this, resultsData);
		try {
			work.doInBackground();
		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
	}
	
	public void updateProgress(int v){
		//System.out.println("Updated");
		this.setProgress(v);
		//System.out.print(resultsData.size());
	}

}
